	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					"// Глобальный pre-request: если есть access_token, подставляем Authorization",
					"const token = pm.collectionVariables.get('access_token');",
					"if (token) {",
					"    if (pm.request.auth && pm.request.auth.type === 'bearer') {",
					"        pm.request.auth.bearer.clear();",
					"        pm.request.auth.bearer.add({ key: 'token', value: token, type: 'string' });",
					"    } else {",
					"        pm.request.headers.upsert({ key: 'Authorization', value: `Bearer ${token}` });",
					"    }",
					"}"
				]
			}
		}
